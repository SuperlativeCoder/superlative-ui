{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "jest": true
    },
    "rules": {
      "strict": 0,
      "max-len": ["error", 200, 4, {"ignoreUrls": true}],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "global-require": 0,
      "no-param-reassign": 0,
      "generator-star-spacing": 0,
      "no-console": 0,
      "class-methods-use-this": 0
    }
}
