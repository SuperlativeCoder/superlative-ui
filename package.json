  "scripts": {
    "build:es2015": "cross-env NODE_ENV=production babel ./components --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./components/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./components --out-dir ./build/es",
    "build:static": "babel-node ./scripts/copy-static.js",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:static",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS.txt",
    "watch-tsc": "antd-tools run watch-tsc"
